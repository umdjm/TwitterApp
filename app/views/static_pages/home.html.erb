
MaxId: <%= @max_id %>, Results: <%= @tweets.count %>, Pages <%= @counter %>
<table>
  <tr>
    <th>From</th>
    <th>Text</th>
    <th>Time</th>
  </tr>

  <% @tweets.each do |tweet| %>
      <tr>
        <td><%= tweet["from_user"] %></td>
        <td><%= tweet["text"] %></td>
        <td><%= tweet["created_at"] %></td>
      </tr>
  <% end %>


</table>